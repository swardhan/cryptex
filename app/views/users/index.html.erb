<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Username</th>
      <th>Level</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <% #rank = @users.map(&:id).index(user.id) %>
      <tr>
        <td><%= user.rank(user) %></td>
        <td><%= user.username %></td>
        <td><%= user.level %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
